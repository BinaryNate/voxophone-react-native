{
  "env": {
    "browser": true,
    "mocha": true,
    "node": true,
    "es6": true
  },
  "globals": {},
  "parserOptions": {
    "sourceType": "module",
    "ecmaFeatures": {
      "modules": true,
      "jsx": true
    }
  },
  "plugins": [
    "react"
  ],
  "rules": {
    "indent": [
      2,
      4,
      {
        "SwitchCase": 1
      }
    ],
    "no-irregular-whitespace": 2,
    "quotes": [
      "error",
      "single",
      { "allowTemplateLiterals": true }
    ],
    "no-use-before-define": 0,
    "no-extend-native": 2,
    "eqeqeq": 2,
    "no-unused-vars": 2,
    "no-undef": 2,
    "no-loop-func": 2,
    "react/jsx-uses-vars": 2
  }
}
